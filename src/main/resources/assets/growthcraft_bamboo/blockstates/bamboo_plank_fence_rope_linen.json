{
  "multipart": [
    {
      "apply": {
        "model": "growthcraft_bamboo:block/bamboo_plank_fence_post"
      }
    },
    {
      "apply": {
        "model": "growthcraft:block/rope_linen_knot",
        "uvlock": false
      },
